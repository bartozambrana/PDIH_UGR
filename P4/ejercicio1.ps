	%% Dibujamos texto que establezca Mi casita.
	/Times-Roman findfont
	10 scalefont
	setfont

	newpath
	100 135 moveto

	gsave
	90 rotate
	(Mi Casita) show
	grestore


%% Dibujamos la página.
showpage