		%% Establecemso el color de la línea azul claro.
		0.05 0.613 0.765 setrgbcolor

		%% Cerramos el objeto a dibujar
		stroke

	%% Dibujamos un cuadrado (ventana derecha).
	newpath
	205 160 moveto
	235 160 lineto
	235 180 lineto
	205 180 lineto
	closepath 

		%% Guardamos el path
		gsave

		%% Establecemos el color de relleno.
		0.75 setgray
